{
  "name": "@bluryar/eslint-config-vue",
  "type": "module",
  "version": "1.1.0",
  "private": false,
  "description": "Extend from [antfu/eslint-config](https://github.com/antfu/eslint-config)",
  "author": "bluryar <bluryarl@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bluryar/configany/tree/main/packages/eslint-config-vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluryar/configany.git"
  },
  "bugs": "https://github.com/bluryar/configany/issues",
  "keywords": [
    "config",
    "eslint",
    "eslintrc"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "bump:patch": "npx bumpp patch --no-commit --no-tag --no-push --yes",
    "bump:minor": "npx bumpp minor --no-commit --no-tag --no-push --yes",
    "bump:major": "npx bumpp major --no-commit --no-tag --no-push --yes",
    "release": "npm publish"
  },
  "dependencies": {
    "@antfu/eslint-config-basic": "^0.39.5",
    "@babel/core": "^7.22.1",
    "@babel/eslint-parser": "^7.21.8",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@unocss/eslint-config": "^0.53.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint-config-alloy": "^5.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsonc": "^2.8.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.14.1",
    "eslint-plugin-yml": "^1.7.0",
    "vue-eslint-parser": "^9.3.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.40.0",
    "eslint": "^8.42.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
