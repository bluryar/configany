{
  "name": "@bluryar/tsconfig",
  "type": "commonjs",
  "version": "1.0.6",
  "private": false,
  "description": "Extend from [vuejs/tsconfig](https://github.com/vuejs/tsconfig)",
  "author": "bluryar <bluryarl@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bluryar/configany/tree/main/packages/tsconfig#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluryar/configany.git"
  },
  "bugs": "https://github.com/bluryar/configany/issues",
  "keywords": [
    "tsconfig"
  ],
  "exports": {
    ".": "./tsconfig.json",
    "./tsconfig.json": "./tsconfig.json",
    "./tsconfig.web.json": "./tsconfig.web.json",
    "./tsconfig.node.json": "./tsconfig.node.json",
    "./*": "./*"
  },
  "main": "tsconfig.json",
  "files": [
    "tsconfig.json",
    "tsconfig.web.json",
    "tsconfig.node.json",
    "README.md"
  ],
  "scripts": {
    "bump:patch": "npx bumpp patch --no-commit --no-tag --no-push --yes",
    "bump:minor": "npx bumpp minor --no-commit --no-tag --no-push --yes",
    "bump:major": "npx bumpp major --no-commit --no-tag --no-push --yes",
    "release": "npm publish"
  },
  "dependencies": {
    "@vue/tsconfig": "^0.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
