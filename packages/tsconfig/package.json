{
  "name": "@bluryar/tsconfig",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "Extend from [vuejs/tsconfig](https://github.com/vuejs/tsconfig)",
  "author": "bluryar <bluryarl@gmail.com>",
  "license": "MIT",
  "keywords": ["tsconfig"],
  "exports": {
    ".": {
      "default": "./tsconfig.json"
    },
    "./tsconfig.json": "./tsconfig.json",
    "./tsconfig.web.json": "./tsconfig.web.json",
    "./tsconfig.node.json": "./tsconfig.node.json",
    "./*": "./*"
  },
  "main": "tsconfig.json",
  "files": [
    "tsconfig.json",
    "tsconfig.web.json",
    "tsconfig.node.json",
    "README.md"
  ],
  "dependencies": {
    "@vue/tsconfig": "^0.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
